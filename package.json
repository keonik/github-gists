{
    "name": "nextjs-typescript-material-ui-eslint-jest",
    "version": "1.0.1",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.3.20",
        "@material-ui/core": "^4.11.4",
        "@prisma/client": "^2.25.0",
        "apollo-datasource-rest": "^0.14.0",
        "apollo-server-micro": "^2.25.1",
        "dayjs": "^1.10.5",
        "graphql": "^15.5.1",
        "lodash.debounce": "^4.0.8",
        "next": "^10.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2"
    },
    "scripts": {
        "build": "prisma generate && next build",
        "dev": "concurrently \"next -p 3001\" \"npm run watch-queries\"",
        "format": "prettier --write . && eslint --fix .",
        "lint": "eslint .",
        "start": "prisma migrate deploy && next start",
        "test": "jest",
        "type-check": "tsc",
        "quality": "npm run type-check && npm run lint && npm run test",
        "generate-types": "graphql-codegen --config codegen.yml",
        "watch-queries": "graphql-codegen --config codegen.yml --watch"
    },
    "devDependencies": {
        "@axe-core/react": "^4.2.0",
        "@babel/core": "^7.13.16",
        "@babel/node": "^7.13.13",
        "@babel/plugin-transform-typescript": "^7.13.0",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-typescript": "^7.13.0",
        "@graphql-codegen/cli": "^1.21.5",
        "@graphql-codegen/typescript": "^1.22.3",
        "@graphql-codegen/typescript-operations": "^1.18.2",
        "@graphql-codegen/typescript-react-apollo": "^2.2.7",
        "@testing-library/dom": "^7.31.0",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "concurrently": "^6.2.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "prettier": "^2.3.0",
        "prisma": "^2.25.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    }
}
